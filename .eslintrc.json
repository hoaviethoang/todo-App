{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes":[
        "error","single"
      ],
      "linebreak-style":"off",
      "camelcase":"off",
      "no-unused-vars": "off"
    }
}
